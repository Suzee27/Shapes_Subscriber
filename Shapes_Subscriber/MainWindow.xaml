<Window x:Class="Shapes_Subscriber.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Shapes_Subscriber"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <DockPanel>
        <TextBlock Text="Shapes Subscriber" DockPanel.Dock="Top" FontSize="30"  FontWeight="Bold"/>
        <Canvas x:Name="canvas" DockPanel.Dock="Bottom" Background="Transparent"/>
    </DockPanel>
</Window>
